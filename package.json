{
  "name": "ern-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for building full stack apps using React/Node/Express",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "iall": "npm i && cd client && npm i",
    "server": "cd server && node index.js",
    "client": "cd client && npm run start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build:client": "cd client && npm run build",
    "start": "node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rchlblns/ern-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rchlblns/ern-boilerplate/issues"
  },
  "homepage": "https://github.com/rchlblns/ern-boilerplate#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
